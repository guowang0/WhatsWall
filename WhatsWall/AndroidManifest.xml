<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.whatswall"
    android:versionCode="15"
    android:versionName="1.0.5" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name="com.whatswall.base.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/App" >
        <activity
            android:name="com.whatswall.ui.LaunchActivity"
            android:label="@string/app_name"
            android:theme="@style/start_theme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.whatswall.service.WWService" >
        </service>

        <activity
            android:name="com.whatswall.ui.WhatsWallActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme_Light_BlackActionBar" >
            </activity>
        
        <activity
            android:name="com.whatswall.ui.RoomWelcomeActivity"
            android:theme="@style/App" >
        </activity>
        <activity
            android:name="com.whatswall.ui.RoomActivity"
            android:theme="@style/Theme_Light_RedActionBar" >
        </activity>
        <activity
            android:name="com.whatswall.ui.RegisterActivity"
            android:theme="@style/Theme_Light_RedActionBar" >
        </activity>
        <activity android:name="com.whatswall.ui.ContentActivity" >
        </activity>
        <activity android:name="com.whatswall.ui.UserInfoActivity" >
        </activity>
        <activity android:name="com.whatswall.ui.PasswordActivity" >
        </activity>
        <activity
            android:name="com.whatswall.ui.LoginActivity"
            android:label="@string/action_login"
            android:theme="@style/GetTo" >
        </activity>
        <activity
            android:name="com.whatswall.ui.RoomPublishActivity"
            android:theme="@style/Theme_Light_RedActionBar" >
        </activity>
        <activity
            android:name="com.whatswall.ui.FavoriteActivity"
            android:theme="@style/Theme_Light_RedActionBar" >
        </activity>
        <activity
            android:name="com.whatswall.ui.ReportActivity"
            android:theme="@style/Theme_Light_RedActionBar" >
        </activity>
        <activity
            android:name="com.whatswall.ui.RoomWelcomeChangeActivity"
            android:theme="@style/Theme_Light_RedActionBar" >
        </activity>
        <activity
            android:name="com.whatswall.ui.ImageViewActivity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.whatswall.ui.ImageViewPhotoActivity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.whatswall.ui.FavoriteNoteChangeActivity"
            android:theme="@style/Theme_Light_RedActionBar" >
        </activity>
        <activity
            android:name="com.whatswall.ui.FeedBackActivity"
            android:theme="@style/Theme_Light_RedActionBar" >
        </activity>
        
    </application>

</manifest>